      "path": "./plugins/fs-watch",
      "manager": "javascript"
    },
    "tauri-plugin-log": {
      "path": "./plugins/log",
      "manager": "rust"
    },
    "tauri-plugin-log-api": {
      "path": "./plugins/log",
      "manager": "javascript"
    },
    "tauri-plugin-localhost": {
